page{
  background-color: #F8F8F8;
}
.partner{
   width: 100%;
   border-top: 1rpx solid #DBDBDB; 
}
.partnerListBox{
  width: 100%;
  box-sizing: border-box;
  padding-left: 20rpx;
  background-color: white;
}
.partnerList{
  width: 100%;
  box-sizing: border-box;
  border-bottom: 1rpx solid #DBDBDB; 
  overflow: hidden;
  font-size: 32rpx;
  padding: 20rpx ;
}
.partnerListLeft{
  width: 100rpx;
  height: 100rpx;
  float: left;
  border-radius: 50%;
  background-color: #DBDBDB;
}
.partnerListLeft image{
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.partnerListRight{
  float: left;
  margin-left: 40rpx;
  margin-top: 30rpx;
}