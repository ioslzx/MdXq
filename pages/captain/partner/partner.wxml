<view class='partner'>
  <view class='partnerListBox'>
    <view class='partnerList' wx:for='{{partnerInfo}}' wx:for-index='index' wx:for-item='item' wx:key='index'>
      <view class='partnerListLeft'>
        <image src='{{item.picture}}'></image>
      </view>
      <view class='partnerListRight'>{{item.nickname}}</view>
    </view>
  </view>
</view>
