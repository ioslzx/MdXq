page{
  background-color: #F8F8F8;
}
.factoryDividendEarnings{
   width: 100%;
   border-top: 1rpx solid #DBDBDB; 
}
/* 头部导航开始 */
.topNavBox{
  width: 100%;
  box-sizing: border-box;
  padding:40rpx 30rpx 0 ; 
  background-color: white;
  font-size: 32rpx;
  overflow: hidden;
  margin-bottom: 20rpx;
}
.navList{
  width: 20%;
  text-align: center;
  padding-bottom: 25rpx;
  float: left;
  margin-right:20%; 
}
.navList:last-child{
  margin-right: 0;
}
.navListActive{
  color: #902D87;
  border-bottom: 1rpx solid #902D87;
}
/* 奖励列表开始 */
.earningsListBox{
  width: 100%;
  padding: 0 20rpx;
  box-sizing: border-box;
  background-color: white;
  font-size: 32rpx;
  margin-bottom: 15rpx;
}
.earningsListTop{
  width: 100%;
  padding: 30rpx 20rpx 15rpx;
  box-sizing: border-box;
  border-bottom: 1rpx solid #DBDBDB;
  letter-spacing: 2rpx;
}
.earningsListBottom{
  width: 100%;
  padding: 40rpx 20rpx;
  box-sizing: border-box;
  overflow: hidden;
}
.earningsListBottom>text:nth-child(1){
  float: left;
}
.earningsListBottom>text:nth-child(2){
  color: #FE4846;
  float: right;
}