.myGiftBagTemplate{
  width: 49%;
  background-color: white;
  font-size: 30rpx;
  float: left;
  margin-top: 10rpx;
  padding-bottom: 30rpx;
}
.myGiftBagTemplate:nth-child(odd){
  margin-right: 2%;
}
.GiftBagImg{
  width: 100%;
  height: 300rpx;
}
.GiftBagImg image{
  width: 100%;
  height: 100%;
}
.GiftBagLine:nth-child(2){
  margin-top: 10rpx;
}
.GiftBagLine:nth-child(3) text:nth-child(1){
  color: #FC4349;
}
.GiftBagLine:nth-child(3) text:nth-child(2){
  float: right;
  color: #737373;
}
.GiftBagLine{
  margin-bottom: 20rpx;
  padding: 0 10rpx;
  overflow: hidden;
}
.buyButton{
  width: 280rpx;
  margin: 0 auto;
  padding: 8rpx 0;
  background-color: #FC4349;
  color: white;
  text-align: center;
}