@import '../newProducts/newProductsTemplate/newProductsTemplate.wxss';
page{
  background-color: rgb(245, 246, 247);
}
.newProductsTop{
  width: 100%;
  height: 320rpx;
}
.newProductsTop image{
  width: 100%;
  height: 100%;
}
.timeUpTimeBox{
  width: 100%;
  box-sizing: border-box;
  padding: 30rpx;
}
.timeUpTimeBox view:nth-child(1){
  color: #707070;
  margin: 0 auto;
  width: 80%;
  text-align: center;
}
.timeBox{
  width: 80%;
  margin: 20rpx auto 0;
  text-align: center;
}
.timeBox text:nth-child(odd){
  width: 40rpx;
  height: 40rpx;
  background-color: #A0A0A0;
  color: white;
  margin-right: 10rpx;
  padding: 0 7rpx ;
}
.timeBox text:nth-child(even){
  margin-right: 10rpx;
}