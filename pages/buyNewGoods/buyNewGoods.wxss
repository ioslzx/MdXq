}
/*
topBanner
*/
.topBanner{
  padding: 20rpx 10rpx;
  background-color: white;
  box-sizing: border-box;
  width: 100%;
  height: 350rpx;
  border-top: 2rpx solid rgb(245, 246, 247);
  border-bottom: 2rpx solid rgb(245, 246, 247);
}
.topBanner image{
  width: 100%;
  height: 100%;
  border-radius: 15rpx;
}
/*
